 Dashboard 2 React contexts\nimport { useMaterialUIController, setDirection } from \"context\";\n\nfunction RTL() {\n  const [, dispatch] = useMaterialUIController();\n  const { sales, tasks } = reportsLineChartData;\n\n  // Changing the direction to rtl\n  useEffect(() => {\n    setDirection(dispatch, \"rtl\");\n\n    return () => setDirection(dispatch, \"ltr\");\n  }, []);\n\n  return (\n    <DashboardLayout>\n      <DashboardNavbar />\n      <MDBox py={3}>\n        <Grid container spacing={3}>\n          <Grid item xs={12} md={6} lg={3}>\n            <MDBox mb={1.5}>\n              <ComplexStatisticsCard\n                color=\"dark\"\n                icon=\"weekend\"\n                title=\"أموال اليوم\"\n                count={281}\n                percentage={{\n                  color: \"success\",\n                  amount: \"+55%\",\n                  label: \"من الأسبوع الماضي\",\n                }}\n              />\n            </MDBox>\n          </Grid>\n          <Grid item xs={12} md={6} lg={3}>\n            <MDBox mb={1.5}>\n              <ComplexStatisticsCard\n                icon=\"leaderboard\"\n                title=\"مستخدمو اليوم\"\n                count=\"2,300\"\n                percentage={{\n                  color: \"success\",\n                  amount: \"+3%\",\n                  label: \"من الأسبوع الماضي\",\n                }}\n              />\n            </MDBox>\n          </Grid>\n          <Grid item xs={12} md={6} lg={3}>\n            <MDBox mb={1.5}>\n              <ComplexStatisticsCard\n                color=\"success\"\n                icon=\"store\"\n                title=\"عملاء جدد\"\n                count=\"34k\"\n                percentage={{\n                  color: \"success\",\n                  amount: \"+1%\",\n                  label: \"من الشهر الماضي\",\n                }}\n              />\n            </MDBox>\n          </Grid>\n          <Grid item xs={12} md={6} lg={3}>\n            <MDBox mb={1.5}>\n 